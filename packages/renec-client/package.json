{
  "name": "@avala/renec-client",
  "version": "0.1.0",
  "description": "RENEC (CONOCER) data harvester and client for Avala",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "renec-harvest": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts src/extractor/cli.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/cli.ts src/extractor/cli.ts --format cjs,esm --dts --watch",
    "harvest": "tsx src/cli.ts",
    "harvest:ec": "tsx src/cli.ts --type ec",
    "harvest:certifiers": "tsx src/cli.ts --type certifier",
    "harvest:centers": "tsx src/cli.ts --type center",
    "harvest:sectors": "tsx src/cli.ts --type sector",
    "extract": "tsx src/extractor/cli.ts",
    "extract:test": "tsx src/extractor/cli.ts --limit 10 --visible",
    "extract:full": "tsx src/extractor/cli.ts --full",
    "extract:resume": "tsx src/extractor/cli.ts --resume",
    "extract:stats": "tsx src/extractor/cli.ts --stats",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "playwright": "^1.40.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.14"
  },
  "peerDependencies": {
    "@prisma/client": "6.1.0"
  },
  "keywords": [
    "renec",
    "conocer",
    "competency-standards",
    "mexico",
    "harvester"
  ],
  "author": "MADFAM",
  "license": "UNLICENSED"
}
